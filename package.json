{
  "name": "tripetto-forms-examples-angular",
  "title": "Tripetto Forms Collector example (Angular)",
  "version": "0.0.0",
  "description": "Demo for implementing the Tripetto Forms collector using Angular.",
  "license": "MIT",
  "private": true,
  "homepage": "https://gitlab.com/tripetto/forms/examples/angular",
  "author": {
    "name": "Mark van den Brink",
    "email": "mark@vandenbr.ink",
    "url": "http://mark.vandenbr.ink"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/tripetto/forms/examples/angular.git"
  },
  "bugs": {
    "url": "https://gitlab.com/tripetto/forms/examples/angular/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "tripetto ./src/assets/form.json",
    "format": "prettier --tab-width 2 --single-quote --print-width 140 --write \"src/**/*.ts\"",
    "build": "ng build",
    "make": "npm run format && npm run lint && ng build -prod",
    "test": "ng serve",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@tripetto/forms-collector": "^0.4.17",
    "core-js": "^2.5.4",
    "prettier": "^1.11.1",
    "rxjs": "^5.5.8",
    "superagent": "^3.8.2",
    "zone.js": "^0.8.25"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@tripetto/forms-editor": "^0.2.2",
    "@types/node": "^9.6.2",
    "@types/superagent": "^3.5.7",
    "codelyzer": "^4.2.1",
    "tripetto-forms-checkbox": "^1.0.14",
    "tripetto-forms-checkboxes": "^1.0.9",
    "tripetto-forms-dropdown": "^1.0.9",
    "tripetto-forms-email": "^1.0.9",
    "tripetto-forms-number": "^1.0.8",
    "tripetto-forms-password": "^1.0.8",
    "tripetto-forms-radiobuttons": "^1.0.8",
    "tripetto-forms-text": "^1.0.8",
    "tripetto-forms-textarea": "^1.0.7",
    "tripetto-forms-url": "^1.0.7",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "tripetto": {
    "providers": [
      "tripetto-forms-checkbox",
      "tripetto-forms-checkboxes",
      "tripetto-forms-dropdown",
      "tripetto-forms-email",
      "tripetto-forms-number",
      "tripetto-forms-password",
      "tripetto-forms-radiobuttons",
      "tripetto-forms-text",
      "tripetto-forms-textarea",
      "tripetto-forms-url"
    ],
    "noFurtherLoading": true
  }
}
