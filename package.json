{
  "name": "tripetto-forms-examples-angular",
  "title": "Tripetto Forms Collector example (Angular)",
  "version": "0.0.0",
  "description": "Demo for implementing the Tripetto Forms collector using Angular.",
  "license": "MIT",
  "private": true,
  "homepage": "https://gitlab.com/tripetto/forms/examples/angular",
  "author": {
    "name": "Mark van den Brink",
    "email": "mark@vandenbr.ink",
    "url": "http://mark.vandenbr.ink"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/tripetto/forms/examples/angular.git"
  },
  "bugs": {
    "url": "https://gitlab.com/tripetto/forms/examples/angular/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "tripetto ./src/assets/form.json",
    "format": "prettier --tab-width 2 --single-quote --print-width 140 --write \"src/**/*.ts\"",
    "build": "ng build",
    "make": "npm run format && npm run lint && ng build -prod",
    "test": "ng serve",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@tripetto/forms-collector": "^0.4.0",
    "core-js": "^2.4.1",
    "prettier": "^1.8.2",
    "rxjs": "^5.4.2",
    "superagent": "^3.6.3",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.7",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@tripetto/forms-editor": "*",
    "@types/node": "~6.0.60",
    "@types/superagent": "^3.5.6",
    "codelyzer": "^3.2.1",
    "tripetto-forms-checkbox": "^1.0.5",
    "tripetto-forms-checkboxes": "^1.0.5",
    "tripetto-forms-dropdown": "^1.0.5",
    "tripetto-forms-email": "^1.0.5",
    "tripetto-forms-number": "^1.0.5",
    "tripetto-forms-password": "^1.0.5",
    "tripetto-forms-radiobuttons": "^1.0.5",
    "tripetto-forms-text": "^1.0.5",
    "tripetto-forms-textarea": "^1.0.5",
    "tripetto-forms-url": "^1.0.5",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "~2.3.3"
  },
  "tripetto": {
    "providers": [
      "tripetto-forms-checkbox",
      "tripetto-forms-checkboxes",
      "tripetto-forms-dropdown",
      "tripetto-forms-email",
      "tripetto-forms-number",
      "tripetto-forms-password",
      "tripetto-forms-radiobuttons",
      "tripetto-forms-text",
      "tripetto-forms-textarea",
      "tripetto-forms-url"
    ],
    "noFurtherLoading": true
  }
}
